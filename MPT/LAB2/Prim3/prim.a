START:	JMP LOAD ;переход к программе
		ORG 2100H;директива размещения программы с адреса 2100
LOAD:	MOV R0,#0X41 ;загрузка в R0 указателя данных
		MOV @R0,#0FFH ;запись в память числа 
		INC R0 ;инкремент указателя
		MOV @R0,#0FFH ;запись в память числа
		INC R0 ;инкремент указателя
		MOV @R0,#1H ;запись в память числа
		MOV R1, #0X55
		MOV R2, #3
			
		CLR C 
		MOV A,#0 
		
LOOP:	MOV A,@R0 
		MOV B,#5 
		MUL AB 
		
		ADDC A, @R1
		MOV @R1, A
		DEC R1
		
		MOV @R1, B
		DEC R0
		
		DJNZ R2,LOOP 	
		
		END